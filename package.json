{
  "name": "option-analysis",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run combine-data && npm run update-all-data",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "update-data": "node scripts/update-data-files.js",
    "watch-data": "node scripts/update-data-files.js --watch",
    "update-darkpool-data": "node scripts/update-darkpool-data-files.js",
    "watch-darkpool-data": "node scripts/update-darkpool-data-files.js --watch",
    "update-all-data": "node scripts/update-all-data-files.js",
    "combine-data": "node scripts/combineDataFiles.js",
    "check-data-status": "node scripts/checkDataStatus.js"
  },
  "dependencies": {
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.3.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
